@model AdministratorApp.Models.Service
@{
    ViewData["Title"] = "ServiceDelete";
}
<div>
    <dl class="dl-horizontal">
        <dt>Service to be removed:</dt>
        <dd>
            @Html.DisplayFor(model => model.Service_Name)
        </dd>
    </dl>
</div>
<div>
    <form asp-controller="Service" asp-action="DeleteService" method="post">
        <div class="form-group">
            <input type="submit" value="Delete" />
        </div>
    </form>
</div>